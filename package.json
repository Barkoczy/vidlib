{
  "name": "vidlib",
  "version": "1.0.0",
  "description": "Video library for videos of an account based on PeerTube REST API with can usage creator portal.",
  "keywords": [
    "express",
    "ejs",
    "bootstrap",
    "typescript",
    "docker",
    "peertube",
    "video library"
  ],
  "author": "Henrich Barkoczy <me@barkoczy.social> (https://barkoczy-coding.com - https://barkoczy.social)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Barkoczy/vidlib.git"
  },
  "license": "MIT",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon src/server.ts ",
    "build": "tsc",
    "lint": "eslint --fix src",
    "lint:fix": "eslint -- --fix src",
    "test": "jest",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "build:webpack:prod": "npx webpack --config webpack.prod.js",
    "build:webpack:dev": "npx webpack --config webpack.dev.js",
    "watch:webpack:dev": "npx webpack --config webpack.dev.js --watch"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-compress": "^1.0.0",
    "express-minify-html-3": "^1.2.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "path": "^0.12.7",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "autoprefixer": "^10.4.13",
    "clean-webpack-plugin": "^4.0.0",
    "cookie-parser": "^1.4.6",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "import": "^0.0.6",
    "jest": "^29.3.1",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^5.0.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.8.0"
  }
}
